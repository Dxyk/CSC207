	@Test
	public void testAverage() {
		FlightDataRecorder fdr = new FlightDataRecorder();
		for (int i = 0; i < 10; i ++) {
			fdr.record((double) 1);
		}
		assertEquals(1, fdr.average(), 0.1);
	}

